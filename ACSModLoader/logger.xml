<log4net>
    <appender name="ModLoaderFile" type="log4net.Appender.FileAppender">
        <file value="ModLoader.log" />
        <appendToFile value="false" />
        <layout type="log4net.Layout.SimpleLayout" />
    </appender>

    <root>
        <level value="ALL" />
        <appender-ref ref="ModLoaderFile" />
    </root>
</log4net>